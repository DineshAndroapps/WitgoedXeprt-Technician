<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>

        <shape>

            <stroke
                android:width="1dp"
                android:color="@color/colorPrimary"
                android:dashWidth="@dimen/dash_width"
                android:dashGap="@dimen/dash_gap" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />

            <corners android:radius="3dp" />
        </shape>
    </item>
</layer-list>