<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>

            <stroke
                android:dashGap="2dp"
                android:dashWidth="3dp"
                android:width="1dp"
                android:color="@color/colorPrimary" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />

            <corners
                android:radius="3dp"/>
        </shape>
    </item>
</layer-list>