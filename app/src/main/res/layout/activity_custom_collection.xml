<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F4CA"
    android:orientation="vertical"
    tools:context=".Activity.PRO.CustomCollection">

    <include
        android:id="@+id/tool_data"
        layout="@layout/lyt_tool" />

    <LinearLayout
        android:id="@+id/ll_date"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="@dimen/height_20"
        android:gravity="center_horizontal"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:id="@+id/card_date"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:cardBackgroundColor="@color/red_300"
            app:cardCornerRadius="20dp"
            app:cardElevation="5dp">

            <EditText
                android:id="@+id/et_date"
                style="@style/edt_layout"
                android:hint="Select Date"
                android:imeOptions="actionNext"
                android:inputType="text"
                android:maxLines="1"
                android:padding="10dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                android:textColor="@color/black"
                android:textColorHint="@color/grey_90" />

            <ImageView
                android:id="@+id/date_picker_actions"
                android:layout_width="40dp"
                android:layout_height="@dimen/_40sdp"
                android:layout_gravity="end"
                android:layout_marginEnd="@dimen/height_10"
                android:layout_weight="0.2"
                android:gravity="center_vertical"
                android:padding="5dp"
                android:src="@drawable/ic_date"
                android:tint="@color/colorPrimary" />

        </androidx.cardview.widget.CardView>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp25"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="-2dp"
            android:layout_marginTop="@dimen/dp15"
            android:background="@drawable/cell_shape_top"
            android:clickable="true"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="4dp">

            <TextView
                android:id="@+id/yest_count"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Lead Collected : 0"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="10sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <LinearLayout
                            style="@style/lyt_ll"
                            android:background="@drawable/cell_shape_top_corner"
                            android:clickable="true"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="@dimen/dp5">

                            <TextView
                                android:id="@+id/AdmissionDone_n"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="Admission"
                                android:textAlignment="center"
                                android:textColor="@color/black"
                                android:textSize="8sp" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp25"
                            android:background="@drawable/bg_yellow"
                            android:clickable="true"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/walk_in_n"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:text="walkin"
                                android:textColor="@color/black"
                                android:textSize="8sp" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp25"
                            android:background="@drawable/cell_red_bottom_corner"
                            android:clickable="true"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/dp3">

                            <TextView
                                android:id="@+id/NotInterested_n"

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:text="Not Interested"
                                android:textColor="@color/black"
                                android:textSize="8sp" />

                        </LinearLayout>


                    </LinearLayout>

                    <!--
                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="end"
                                    android:background="@android:color/darker_gray" />
                    -->

                </LinearLayout>

                <!--
                        <View
